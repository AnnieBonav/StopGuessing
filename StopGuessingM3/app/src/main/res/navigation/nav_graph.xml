<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mealPlansFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.anniebonav.stopguessingm3.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_mealPlansFragment"
            app:destination="@id/mealPlansFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.anniebonav.stopguessingm3.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/mealPlansFragment"
        android:name="com.anniebonav.stopguessingm3.MealPlansFragment"
        android:label="Meal Plans"
        tools:layout="@layout/fragment_meal_plans">
        <action
            android:id="@+id/action_mealPlansFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>